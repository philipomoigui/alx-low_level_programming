#include "holberton.h"

/**
 * _strcat - concatenates two strings
 * @dest: destination string
 * @src: source string
 *
 * Return: the concatenated characters
 */

char *_strcat(char *dest, char *src)
{
int i, j;
char *p;

for (i = 0; dest[i] != '\0'; i++)
;

for (x = 0; src[x] != '\0'; x++)
;

for (j = 0; dest[j] != '\0'; j++)
{
p[j] = dest[j];
p[i] = src[j]
i++;
}

dest[x] = '\0';
p = dest;
return (p);
}
